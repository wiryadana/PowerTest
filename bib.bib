	keywords = {Humans, Diagnosis, Differential, Female, Male, Infections, Adult, Anti-Bacterial Agents, C-Reactive Protein, Fever, Hematopoietic Stem Cell Transplantation, Middle Aged, Neutropenia, Predictive Value of Tests, Prognosis},
	pages = {741--744},
